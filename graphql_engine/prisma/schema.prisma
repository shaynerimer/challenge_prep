datasource db {
  provider = "sqlite"
  url      = "file:../../data/order_database.db"
}

generator client {
  provider = "prisma-client-js"
}

model Order {
  id                 Int      @id @default(autoincrement())
  productId          String
  orderQty           Int
  confirmationNumber String   @unique
  status             String   @default("PROCESSING")
  createdAt          DateTime @default(now())
}
